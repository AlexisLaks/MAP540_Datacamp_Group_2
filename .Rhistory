amazonMagic <- function(urlList, delay = 2){
numUrls <- length(urlList)
output <- NULL
for(i in 1:numUrls){
url <- urlList[i]
reviews <- retrieveReviews(url, delay)
output <- rbind(output, reviews)
}
output
}
